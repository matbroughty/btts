<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://appengine.google.com/ns/1.0 ">
    <application>broughtybtts</application>
    <version>1</version>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>

    <static-files>
        <include path="/favicon.ico"/>
        <include path="/js/vendor/**.js" />
        <include path="/js/**.js" />
        <include path="/css/**.css" />
        <include path="/**.html" />
    </static-files>



</appengine-web-app>
